{
  "name": "@betapredit/backend",
  "version": "1.0.0",
  "description": "Backend API para plataforma de apuestas deportivas",
  "main": "dist/index.js",
  "scripts": {
    "test:apis": "node scripts/test-apis.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src --ext .ts",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "verify-db": "node scripts/verify-supabase-connection.js",
    "verify-oauth": "node scripts/verify-google-oauth.js",
    "verify-prisma": "node scripts/verify-prisma-supabase.js",
    "test:external-bets": "node scripts/test-external-bets-api.js",
    "test:deploy": "node scripts/test-backend-deploy.js",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:status": "prisma migrate status",
    "validate-predictions": "tsx src/scripts/validate-prediction-accuracy.ts",
    "validate-all-real-data": "tsx src/scripts/validate-all-real-data.ts",
    "test:real-data": "jest --testPathPattern=real-data --testTimeout=30000",
    "test:integration": "jest --testPathPattern=integration --testTimeout=30000"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@sentry/node": "^10.29.0",
    "@sentry/profiling-node": "^10.29.0",
    "@sentry/react": "^10.29.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.87.0",
    "@types/stripe": "^8.0.416",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fastify": "^4.24.3",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "socket.io": "^4.5.4",
    "speakeasy": "^2.0.0",
    "stripe": "^20.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
